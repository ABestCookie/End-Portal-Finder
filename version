    OS=0x40004,
    fileType=0x1,
    subtype=0x0,
    date=(0, 0)
  ),
  kids=[
    StringFileInfo([
      StringTable(
        '040904B0', [
          StringStruct('CompanyName', 'ABestCookie Studio'),
          StringStruct('FileDescription', '終界祭壇尋找器 GUI 應用程式'),
