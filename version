          StringStruct('ProductName', '終界祭壇尋找器'),
          StringStruct('ProductVersion', '1.2.0.0'),
          StringStruct('LegalCopyright', 'Copyright © 2025 ABestCookie'),
        ])
    ]),
    VarFileInfo([VarStruct('Translation', [1033, 1200])])
  ]
)

